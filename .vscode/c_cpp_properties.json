{
  "env": {
    "ROOT0": "C:/Users/zimme/Documents",
    "ROOT": "${ROOT0}/arduino-1.8.19/hardware/teensy/avr",
    "LOCAL_LIBRARIES": "${ROOT0}/Arduino/libraries",
    "COMMON_DEFINES": [ "TEENSYDUINO=156",
      "USB_SERIAL",
      "LAYOUT_US_ENGLISH",
      "ARDUINO"
    ],
    "COMMON_LIBS": [
      "src/**",
      "${ROOT}/libraries/Audio/**",
      "${ROOT}/libraries/SPI/**",
      "${ROOT}/libraries/Time/**",
      "${ROOT}/libraries/SD/**",
      "${ROOT}/libraries/SdFat/**"
    ]
  },
  "configurations": [
    { "name": "Teensy 36",
      "includePath": [
        "${COMMON_LIBS}",
        "${ROOT}/cores/teensy3/**"
      ],
      "forcedInclude": [],
      "defines": [
        "__MK66FX1M0__",
        "ARDUINO_TEENSY36",
        "F_CPU=96000000",
        "${COMMON_DEFINES}"
      ],
      "compilerPath": "${ROOT}/../../tools/arm/bin/arm-none-eabi-gcc.exe",
      "intelliSenseMode": "gcc-arm",
      "cppStandard": "gnu++14"    
    },
    { "name": "Teensy 40",
      "includePath": [
        "${COMMON_LIBS}",
        "${ROOT}/cores/teensy4/**"
      ],
      "forcedInclude": [],
      "defines": [
        "__IMXRT1062__",
        "ARDUINO_TEENSY40",
        "F_CPU=600000000",
        "${COMMON_DEFINES}"
      ],
      "compilerPath": "${ROOT}/../../tools/arm/bin/arm-none-eabi-gcc.exe",
      "intelliSenseMode": "gcc-arm",
      "cppStandard": "gnu++14"    
    },
    { "name": "Teensy 41",
      "includePath": [
        "${COMMON_LIBS}",
        "${ROOT}/cores/teensy4/**"
      ],
      "forcedInclude": [],
      "defines": [
        "__IMXRT1062__",
        "ARDUINO_TEENSY41",
        "F_CPU=600000000",
        "${COMMON_DEFINES}"
      ],
      "compilerPath": "${ROOT}/../../tools/arm/bin/arm-none-eabi-gcc.exe",
      "intelliSenseMode": "gcc-arm",
      "cppStandard": "gnu++14"    
    }
  ],
  "version": 4
}